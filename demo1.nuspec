<?xml version="1.0"?>
<package >
  <metadata>
    <!-- Required elements -->
    <id>demo1</id>
    <version>1.0.0</version>
    <description>Create NuGet package for Component-demo1 on TeamCity</description>
    <authors>Aki</authors>

    <!-- Optional elements -->
    <title>Component-demo1</title>
    <owners>Peraso Technologies Inc.</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <releaseNotes>Demo1</releaseNotes>
    <copyright>Copyright 2018 Peraso Technologies Inc.</copyright>
    <tags>Tag1</tags>

    <!-- Create dependencies if this package depends on other package(s).
         Avoid circular dependency -->
    <dependencies>
      <group>
        <!-- .nupkg demo2 and demo3 must be created before installing/downloading demo1
             Each dependency has attributes of id, version... 
             We currently use the highest version which is defined in NuGet.Config -->
        <!-- <dependency id="demo2" version="1.0.0" />  -->
        <dependency id="demo2" />
	      <dependency id="demo3" />
      </group>
    </dependencies>
  </metadata>
  
  <!-- specify which assembly and content files to include in the package -->
  <files>
    <file src="*.txt" />
  </files>
	
</package>
